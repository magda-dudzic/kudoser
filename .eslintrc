{
    "extends": "@react-native-community",
    "plugins": ["simple-import-sort"],
    "rules": {
        "quotes":[2, "single", {"avoidEscape": true}],
        "prettier/prettier": ["error", { "singleQuote": true }],
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    }
}
